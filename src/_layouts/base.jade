head
  meta(charset='utf-8')
  meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
  title tcd-badminton
  meta(name='description', content= 'Trinity College Dublin Badminton Website')
  meta(name='viewport', content='width=device-width, initial-scale=1.0')
  meta(name='keywords', content='tcd, Trinity College Dublin, Badminton, tcd badminton')
